<% types = variables[0] %>
<%= @title %>

<% types.each do |t| %>
  <% @count[1].each_pair do |unit, qty| %>
    <% if @rel.unitlist[unit].type == t %>
      <%= "#{unit} (#{qty})" %>
      
      <% @rel.unitlist[unit].children.each_pair do |child, qty2| %>
        <% s_l = @rel.unitlist[unit].ship_loose[@rel.unitlist[child].name] %>
        <%="  (#{(qty.to_i * qty2.to_i).to_s}) [X #{qty2}]  #{child} \n" %>
        <%="  ##[S/L: #{s_l}]" unless s_l == nil %>
      <% end %>
      
    <% end %>
  <% end %>
  
  
<% end %>